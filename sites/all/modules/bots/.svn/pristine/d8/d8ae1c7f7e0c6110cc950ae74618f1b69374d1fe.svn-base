        <ns1:dbRetryMethod>QUADRATIC</ns1:dbRetryMethod>
        <ns1:dbRetryBase>1000</ns1:dbRetryBase>
        <ns1:dbRetryStep>2000</ns1:dbRetryStep>
        <ns1:maxRunTime>86100</ns1:maxRunTime>
