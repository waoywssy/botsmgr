        <ns1:proxyPort>8888</ns1:proxyPort>
        <ns1:proxyUser></ns1:proxyUser>
        <ns1:proxyPassword></ns1:proxyPassword>
        <ns1:maxPageHitsPerHour>0</ns1:maxPageHitsPerHour>
